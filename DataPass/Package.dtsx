                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH MIS_Report AS 
(
	---MIS Report
	SELECT 
	a.Branch_Name, a.Branch_Officer, count(a.Branch_Name) No_Clients,a.gender,sum(a.age)/count(a.Branch_Name) Avg_Age
	FROM (
	SELECT 
	concat(rtrim(d.CenterName),'(',d.CenterId,')') Branch_Name,concat(rtrim(a.NameTitle),' ',rtrim(a.Longname)) Full_name,
	a.CustNo,c.CRS Branch_Officer, b.SexCode gender,dbo.fc_age(DOB) age 
	FROM D009011 a INNER JOIN D009012 b ON a.CustNo=b.CustNo AND a.IndOth=1
	INNER JOIN D009015 c ON a.CustNo=c.CustNo
	INNER JOIN D001006 d ON c.CenterId=d.CenterId
	) a
	GROUP BY a.Branch_Name, a.Branch_Officer,a.gender
),
--SELECT * FROM MIS_Report
   		Savings_Status AS
		(
		SELECT concat(rtrim(b.CenterName),'(',b.CenterId,')') Branch_Name,sum(c.ShdClrBalFcy) Total_Savings,
		CASE 
			WHEN  sum(c.ShdClrBalFcy)&gt;200000 THEN 'No Risk'
			WHEN  sum(c.ShdClrBalFcy)BETWEEN 200000 AND 100000 THEN 'Risk'
			WHEN  sum(c.ShdClrBalFcy)&lt;200000 THEN ' High Risk'
			ELSE 'NA'
		END AS Savings_Status
		FROM D009015 a JOIN D001006 b ON a.CenterId=b.CenterId AND a.IndOth=1
		INNER JOIN d009022 c ON a.CustNo=c.CustNo
		WHERE left(c.PrdAcctId,2)='13' 
		GROUP BY concat(rtrim(b.CenterName),'(',b.CenterId,')')
		),
--SELECT * FROM Savings_Status


		Loan_Information  AS
		(
		SELECT 
		concat(rtrim(b.CenterName),'(',b.CenterId,')') Branch_Name, sum(d.DisbursedAmtFcy) Total_Loan_Amount,
		abs(sum(c.ShdClrBalFcy)) Total_Loan_OS, sum(d.DisbursedAmtFcy)+sum(c.ShdClrBalFcy) Recoverable_Amount
		FROM 
		D009015 a JOIN D001006 b ON a.CenterId=b.CenterId AND a.IndOth=1
		INNER JOIN d009022 c ON a.CustNo=c.CustNo
		INNER JOIN d030003 d ON d.PrdAcctId=c.PrdAcctId AND c.LBrCode=d.LBrCode
		WHERE left(c.PrdAcctId,2)='23' 
		GROUP BY concat(rtrim(b.CenterName),'(',b.CenterId,')')
		)
		
SELECT 
a.*,b.Total_Savings,b.Savings_Status,c.Total_Loan_Amount,c.Total_Loan_OS,c.Recoverable_Amount
FROM 
MIS_Report a INNER JOIN  Savings_Status b ON a.Branch_Name=b.Branch_Name
INNER JOIN Loan_Information c ON c.Branch_Name=a.Branch_Name</property>
